<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Evercoding Vacations" parallel="classes" thread-count="5">
    <parameter name="language" value="en"/>
    <test name="Login Test">
        <parameter name="appURL" value="http://vacations.evercoding.com/"/>
        <classes>
            <class name="fwkTests.LoginTest"/>
        </classes>
    </test>
    <test name="Employee Tests">
    	<parameter name="appURL" value="http://vacations.evercoding.com/"/>
    	<classes>
    		 <class name="fwkTests.EmployeeTests"/>
    	</classes>
    </test>
</suite>
